<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jdssdm="urn:int:nato:standard:mip:jdssdm:1.1" xmlns:jc3iedm="urn:int:nato:standard:mip:jc3iedm:3.0.2:oo:2.2" targetNamespace="urn:int:nato:standard:mip:jdssdm:1.1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<annotation>
		<documentation xml:lang="en">XML schema defined by AC/255 Land Capability Group 1 (LCG/1), C4I and Systems Architecture WG  - Editors: Jean Demers, jdemers@rheinmetall.ca, Rheinmetall Canada , Marco van der Meijden, marco@integrationbydesign.nl, Integration By Design</documentation>
	</annotation>
	<annotation>
		<documentation>ECP-4677-4: Added optional style attribute to SktechObjectType 15-10-1025</documentation>
	</annotation>
	<annotation>
		<documentation>ECP-4677-1.1: Removed optional OID attribute from ConceptualState in IEDType 30-11-2015</documentation>
	</annotation>
	<annotation>
		<documentation>ECP-4677-3: Added optional IsUpdate attribute to OverlayType 15-10-2015</documentation>
	</annotation>
		<annotation>
		<documentation>ECP-4677-6: Made all elements named ExtendedObject xsd:anyType 10-3-2016</documentation>
	</annotation>
	<import namespace="urn:int:nato:standard:mip:jc3iedm:3.0.2:oo:2.2" schemaLocation="JC3IEDM-3.0.2-MergeSchemas.xsd"/>
	<complexType name="AbstractMessage">
		<annotation>
			<documentation xml:lang="en">Provides common attributes for the message </documentation>
		</annotation>
		<attribute name="ReportingDatetime" type="jc3iedm:DatetimeTypeFix18">
			<annotation>
				<documentation>The reporting date time that is to be used for any ReportingDateTime attribute inside the message that is not specified.</documentation>
			</annotation>
		</attribute>
	</complexType>
	<complexType name="ReceiptMsgType">
		<annotation>
			<documentation xml:lang="en">Used to inform about the message reception.</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:AbstractMessage">
				<sequence>
					<element name="ReceiptMsg">
						<simpleType>
							<restriction base="token">
								<enumeration value="RECEIPT_NOTIFICATION"/>
								<enumeration value="RECIPIENT_BUSY"/>
							</restriction>
						</simpleType>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="IdentificationMsgType">
		<annotation>
			<documentation xml:lang="en">Complete identification of friendly Unit. used as reference for Presence messages.</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:AbstractMessage">
				<sequence>
					<element name="Unit" type="jdssdm:UnitIdentificationType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>Squad / section.</documentation>
						</annotation>
					</element>
					<element name="Organisation-MilitaryPost" type="jdssdm:Organisation-MilitaryPostIdentificationType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>Soldier and role</documentation>
						</annotation>
					</element>
					<element name="Materiel" type="jdssdm:MaterielIdentificationType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>Vehicle and weapon</documentation>
						</annotation>
					</element>
					<element name="OrganisationStructure" type="jdssdm:OrganisationStructureType" minOccurs="0">
						<annotation>
							<documentation>The hierarchical configuration of a specific root ORGANISATION where the configuration is specified by reference to a set of associations between instances of OBJECT-ITEM.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="PresenceMsgType">
		<annotation>
			<documentation xml:lang="en">Used to keep a commander informed on the deployment of all military units/formations /installations within his area of responsibility which both are and are not under his command (APP-11 definition).</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:AbstractMessage">
				<sequence>
					<element name="Unit" type="jdssdm:LOC-ObjectItemRef" minOccurs="0" maxOccurs="unbounded"/>
					<element name="Organisation-MilitaryPost" type="jdssdm:LOC-ObjectItemRef" minOccurs="0" maxOccurs="unbounded"/>
					<element name="Materiel" type="jdssdm:LOC-ObjectItemRef" minOccurs="0" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ContactSightingMsgType">
		<annotation>
			<documentation xml:lang="en">To standardise the method used to report contact with the enemy (enemy aware) or sighting of the enemy (enemy unaware).</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:AbstractMessage">
				<choice maxOccurs="unbounded">
					<element name="Unit" type="jdssdm:UnitType"/>
					<element name="Organisation-MilitaryPost" type="jdssdm:Organisation-MilitaryPostType">
						<annotation>
							<documentation>Soldier and role</documentation>
						</annotation>
					</element>
					<element name="Materiel" type="jdssdm:MaterielType">
						<annotation>
							<documentation>Vehicle or weapon</documentation>
						</annotation>
					</element>
					<element name="IED" type="jdssdm:IEDType">
						<annotation>
							<documentation>IED (Improvised explosive device).</documentation>
						</annotation>
					</element>
					<element name="IEDCache" type="jdssdm:IEDCacheType">
						<annotation>
							<documentation>IED Cache</documentation>
						</annotation>
					</element>
					<element name="Facility" type="jdssdm:FacilityType">
						<annotation>
							<documentation>Building. FoxHole.</documentation>
						</annotation>
					</element>
					<element name="MilitaryObstacle" type="jdssdm:MilitaryObstacleType">
						<annotation>
							<documentation> Road block.</documentation>
						</annotation>
					</element>
					<element name="MineFieldLand" type="jdssdm:MineFieldLandType">
						<annotation>
							<documentation>Minefield</documentation>
						</annotation>
					</element>
					<element name="ActionEvent" type="jdssdm:ActionEventType">
						<annotation>
							<documentation>Axis Of Attack - Enemy Confirmed   Ambush, Assassination, Murder, Execution, Bombing, Sniping, Demonstration, Refugees</documentation>
						</annotation>
					</element>
					<element name="ExtendedObject" type="xsd:anyType"/>
				</choice>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="OverlayMsgType">
		<annotation>
			<documentation xml:lang="en">Used to exchange a Graphic overlay.</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:AbstractMessage">
				<sequence>
					<element name="Overlay" type="jdssdm:OverlayType"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="GenInfoMsgType">
		<annotation>
			<documentation xml:lang="en">Used to exchange general text messages</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:AbstractMessage">
				<sequence>
					<element name="TextItem" type="jdssdm:TextItemType"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="CasevacMsgType">
		<annotation>
			<documentation>Used to request medical casualty evacuation support for single and multiple evacuation and by whatever means.</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:AbstractMessage">
				<choice>
					<element name="ActionTask-MEDEVC" type="jdssdm:ActionTask-MEDEVCType">
						<annotation>
							<documentation>The capability for timely and efficient movement of patients while providing en route medical care to and between medical treatment facilities.</documentation>
						</annotation>
						<!-- Fixed ActivityCode: MEDEVC -->
					</element>
					<element name="Reply">
						<complexType>
							<sequence>
								<element name="ActionTaskStatus">
									<complexType>
										<sequence>
											<element name="ApprovalIndicatorCode" type="jc3iedm:ActionTaskStatusApprovalIndicatorCode">
												<annotation>
													<documentation>Values YES, NO</documentation>
												</annotation>
											</element>
										</sequence>
									</complexType>
								</element>
								<element name="Comments" type="jdssdm:Comment" minOccurs="0"/>
							</sequence>
							<attribute name="OID" use="required">
								<annotation>
									<documentation>The OID of the ActionTask-MEDEVC that the reply refers to</documentation>
								</annotation>
							</attribute>
						</complexType>
					</element>
				</choice>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="SketchMsgType">
		<annotation>
			<documentation>Used to exchange generic graphics such as poinys, lines and areas.</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:AbstractMessage">
				<sequence maxOccurs="unbounded">
					<element name="SketchObject" type="jdssdm:SketchObjectType"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="CoordinationMsgType">
		<annotation>
			<documentation>Used to exchange ActionTasks, ControlFeatures and Boundary Lines</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:AbstractMessage">
				<choice maxOccurs="unbounded">
					<element name="ControlFeature" type="jdssdm:ControlFeatureType">
						<annotation>
							<documentation>Points: Waypoint, Point of interest, Single Target, Aim Point, Entry Point, Contact point, Coordination Point, Linkup Point    Line:  FLOT, Final Coordination Line,  Limt of Advance, Line of Departure, Line of Contact, Phase Line                                    Area: Landing Zone, Pickup Zone, Assault Position, Objective, Named Area of Interest, Unexploded Ordnance Area  Corridor2Pt: Assault By Fire pos     Fan: Support By Fire pos</documentation>
						</annotation>
					</element>
					<element name="ActionTask" type="jdssdm:ActionTaskType">
						<annotation>
							<documentation>Point: Destroy, Interdict, Neutralized.               FanArea: Secure, Seize.      Line2PT: Fix.                   Corridor2PT:  Block, Clear, Contain, Conter attack, Direction of Supporting Attack, Friendly Direction of Main Attack.</documentation>
						</annotation>
					</element>
					<element name="ControlFeature-Boundary" type="jdssdm:ControlFeature-BoundaryType">
						<annotation>
							<documentation>Lateral Boundary, Forward Boundary</documentation>
						</annotation>
					</element>
					<element name="ExtendedObject" type="xsd:anyType"/>
				</choice>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="NBCMsgType">
		<annotation>
			<documentation>Used to provide the observer's initial report giving basic data on a single biological, chemical or nuclear attack.
</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:AbstractMessage">
				<sequence>
					<element name="CbrnEvent" type="jdssdm:CBRNEventType">
						<annotation>
							<documentation>An ACTION-EVENT that involves chemical, biological, radiological or nuclear materiel individually or in combination.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="EntityAttributes" abstract="true">
		<annotation>
			<documentation>Superclass that defines a set of common attributes for objects</documentation>
		</annotation>
		<sequence>
			<element name="OID" type="jdssdm:EntityOID"/>
			<element name="CreatorId" type="jdssdm:EntityReference" minOccurs="0">
				<annotation>
					<documentation>The globally unique object identifier of the organisation which created that object.</documentation>
				</annotation>
			</element>
		</sequence>
		<attribute name="EndDateTime" type="jc3iedm:DatetimeTypeFix18">
			<annotation>
				<documentation>The period of effectiveness of the object is terminated at the specifed DateTime.</documentation>
			</annotation>
		</attribute>
		<attribute name="Deleted" type="boolean">
			<annotation>
				<documentation>The perid of effectiveness of the object is terminated.</documentation>
			</annotation>
		</attribute>
	</complexType>
	<complexType name="EntityOID" abstract="false">
		<simpleContent>
			<extension base="jdssdm:JDSSDMKeyType">
				<attribute name="OIDType">
					<annotation>
						<documentation>Indicate the use of other identifier types than the default MIP key as OID</documentation>
					</annotation>
					<simpleType>
						<restriction base="string">
							<maxLength value="30"/>
						</restriction>
					</simpleType>
				</attribute>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="AbstractObjectItem" abstract="true">
		<annotation>
			<documentation>Abstract class introduced to capture common definitons.</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:EntityAttributes">
				<sequence>
					<element name="NameText" type="jc3iedm:TextTypeVar100">
						<annotation>
							<documentation xml:lang="en">The character string assigned to represent a specific OBJECT-ITEM.</documentation>
						</annotation>
					</element>
					<!--element name="AffiliationGeopolitical-Code" type="jc3iedm:AffiliationGeopoliticalCode" default="NOS">
				<annotation>
					<documentation xml:lang="en">The specific value that represents the identification of the independent first-level geographic-political area and its dependencies, areas of quasi-independence, and areas with special unrecognised sovereignty, including outlying and disputed areas.</documentation>
				</annotation>
			</element-->
					<element name="Comment" type="jdssdm:Comment" minOccurs="0">
						<annotation>
							<documentation>Remarks added.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="AbstractObjectType" abstract="true">
		<annotation>
			<documentation>Superclass of types that are part of an object(item)</documentation>
		</annotation>
		<sequence>
			<element name="OID" type="jdssdm:EntityOID">
				<annotation>
					<documentation>The globally unique object identifier.</documentation>
				</annotation>
			</element>
			<element name="CreatorId" type="jdssdm:EntityReference" minOccurs="0">
				<annotation>
					<documentation>The globally unique object identifier of the organisation which created that object.</documentation>
				</annotation>
			</element>
			<element name="NameText" type="jc3iedm:TextTypeVar100">
				<annotation>
					<documentation xml:lang="en">The character string assigned to represent a specific OBJECT-TYPE.</documentation>
				</annotation>
			</element>
		</sequence>
		<attribute name="ReportingDatetime" type="jc3iedm:DatetimeTypeFix18"/>
	</complexType>
	<complexType name="AbstractAction" abstract="true">
		<annotation>
			<documentation>Abstract class introduced to capture common definitons.</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:EntityAttributes">
				<sequence>
					<element name="NameText" type="jc3iedm:TextTypeVar50" minOccurs="0">
						<annotation>
							<documentation xml:lang="en">The character string assigned to represent a specific ACTION.</documentation>
						</annotation>
					</element>
					<element name="Comment" type="jdssdm:Comment" minOccurs="0">
						<annotation>
							<documentation>Remarks added.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="OrganisationStructureType">
		<complexContent>
			<extension base="jdssdm:EntityAttributes">
				<sequence>
					<element name="NameText" type="jc3iedm:TextTypeVar100">
						<annotation>
							<documentation xml:lang="en">The character string assigned to represent a specific OBJECT-ITEM.</documentation>
						</annotation>
					</element>
					<element name="Unit" type="jdssdm:OrgStrucObjectItemAssociationUnit">
						<annotation>
							<documentation>Root organisation.</documentation>
						</annotation>
					</element>
				</sequence>
				<attribute name="ReportingDatetime" type="jc3iedm:DatetimeTypeFix18"/>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="Comment">
		<annotation>
			<documentation>Used to provide additional textual comments on objects.</documentation>
		</annotation>
		<sequence>
			<element name="CommentText" type="jdssdm:TextTypeVarInf">
				<annotation>
					<documentation xml:lang="en">The character string assigned to represent a general remark about a specific OBJECT-ITEM or ACTION</documentation>
				</annotation>
			</element>
		</sequence>
		<attribute name="ReportingDatetime" type="jc3iedm:DatetimeTypeFix18"/>
	</complexType>
	<complexType name="OrgStrucObjectItemAssociationUnit">
		<sequence>
			<element name="OID" type="jdssdm:EntityReference"/>
			<element name="Subordinates" type="jdssdm:OrgStrucObjectItemAssociation" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="OrgStrucObjectItemAssociation">
		<annotation>
			<documentation>When child is Unit or OtherOrg, cat = CMDCTL. When child is Materiel, cat = ISOWNR.</documentation>
		</annotation>
		<choice maxOccurs="unbounded">
			<element name="Unit" type="jdssdm:OrgStrucObjectItemAssociationUnit"/>
			<element name="Organisation-MilitaryPost">
				<complexType>
					<sequence>
						<element name="OID" type="jdssdm:EntityReference"/>
					</sequence>
				</complexType>
			</element>
			<element name="Materiel">
				<complexType>
					<sequence>
						<element name="OID" type="jdssdm:EntityReference"/>
					</sequence>
				</complexType>
			</element>
		</choice>
	</complexType>
	<complexType name="CBRNEventType">
		<annotation>
			<documentation>Simplied representation of a CBRN Event.</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:OVL-AbstractAction">
				<sequence>
					<choice>
						<element name="GeographicPoint" type="jdssdm:GeographicPointLocation">
							<annotation>
								<documentation>An ABSOLUTE-POINT that has its position specified with respect to the surface of the World Geodetic System 1984 (WGS 84) ellipsoid.</documentation>
							</annotation>
						</element>
						<element name="PolygonArea" type="jdssdm:PolygonAreaLocation"/>
					</choice>
					<element name="CategoryCode" type="jdssdm:CBRNEventConceptualCategoryCode">
						<annotation>
							<documentation xml:lang="en">The specific value that represents the class of CBRN-EVENT. It contains the following values: NKN, BIO, CHEM, NUC</documentation>
						</annotation>
					</element>
					<element name="SubCategoryCode" type="jdssdm:CBRNEventConceptualSubCategoryCode">
						<annotation>
							<documentation>The specifci value that represents a further catgeorisation of the CBRN Event. It contains the following values: NKN, ALARM, ATTACK, ROTA</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="Marking">
		<annotation>
			<documentation xml:lang="en">The technique of indicating the position of an ACTION-OBJECTIVE-ITEM at a given time for the benefit of a using ORGANISATION.</documentation>
		</annotation>
		<sequence>
			<element name="MethodCode" type="jc3iedm:ActionObjectiveItemMarkingMethodCode" minOccurs="0">
				<annotation>
					<documentation xml:lang="en">The specific value that represents the method of marking a position.</documentation>
				</annotation>
			</element>
			<element name="PanelShapeCode" type="jc3iedm:ActionObjectiveItemMarkingPanelShapeCode" minOccurs="0">
				<annotation>
					<documentation xml:lang="en">The specific value that represents the shape of the marking panel.</documentation>
				</annotation>
			</element>
			<element name="RecognitionSignalColourCode" type="jc3iedm:ActionObjectiveItemMarkingRecognitionSignalColourCode" minOccurs="0">
				<annotation>
					<documentation xml:lang="en">The specific value that represents the coloration of the marking signal.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="OVL-AbstractObjectItem">
		<annotation>
			<documentation>Abstract class introduced to capture common definitons.</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:AbstractObjectItem">
				<sequence>
					<element name="HostilityStatusCode" type="jdssdm:HostilityStatusCode">
						<annotation>
							<documentation>A record of the perceived hostility classification of a specific OBJECT-ITEM.</documentation>
						</annotation>
					</element>
					<element name="AffiliationGeopolitical-Code" type="jdssdm:AffiliationGeopolitical-Code" minOccurs="0">
						<annotation>
							<documentation xml:lang="en">The specific value that represents the identification of the independent first-level geographic-political area and its dependencies, areas of quasi-independence, and areas with special unrecognised sovereignty, including outlying and disputed areas.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="OVL-AbstractAction" abstract="true">
		<annotation>
			<documentation>Abstract class introduced to capture common definitons.</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:AbstractAction"/>
		</complexContent>
	</complexType>
	<complexType name="LOC-ObjectItemRef">
		<annotation>
			<documentation>Represents a reference to an ObjectItem. Used in the presence message.</documentation>
		</annotation>
		<sequence>
			<element name="OID" type="jdssdm:EntityReference"/>
			<element name="GeographicPoint" type="jdssdm:GeographicPointLocation">
				<annotation>
					<documentation>An ABSOLUTE-POINT that has its position specified with respect to the surface of the World Geodetic System 1984 (WGS 84) ellipsoid.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="HostilityStatusCode">
		<annotation>
			<documentation>ObjectIemHostilityStatusCode with ReportingDateTime.</documentation>
		</annotation>
		<simpleContent>
			<extension base="jc3iedm:ObjectItemHostilityStatusCode">
				<attribute name="ReportingDatetime" type="jc3iedm:DatetimeTypeFix18"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="AffiliationGeopolitical-Code">
		<annotation>
			<documentation>AffiliationGeopoliticalCode with ReportingDateTime.</documentation>
		</annotation>
		<simpleContent>
			<extension base="jc3iedm:AffiliationGeopoliticalCode">
				<attribute name="ReportingDatetime" type="jc3iedm:DatetimeTypeFix18"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="LocationType">
		<annotation>
			<documentation>Represents all possible location types that are supported by the JDSSDM</documentation>
		</annotation>
		<choice>
			<element name="GeographicPoint" type="jdssdm:GeographicPointLocation">
				<annotation>
					<documentation>An ABSOLUTE-POINT that has its position specified with respect to the surface of the World Geodetic System 1984 (WGS 84) ellipsoid.</documentation>
				</annotation>
			</element>
			<element name="Line" type="jdssdm:LineLocation"/>
			<element name="Line2PT" type="jdssdm:Line2PTLocation"/>
			<element name="PolygonArea" type="jdssdm:PolygonAreaLocation"/>
			<element name="FanArea" type="jdssdm:FanAreaLocation"/>
			<element name="Corridor2PT" type="jdssdm:CorridorArea2PtLocation"/>
		</choice>
	</complexType>
	<complexType name="FacilityType">
		<annotation>
			<documentation>XML Complex type used to define a Facility.</documentation>
		</annotation>
		<complexContent>
			<extension base="jdssdm:OVL-AbstractObjectItem">
				<sequence>
					<element name="GeographicPoint" type="jdssdm:GeographicPointLocation">
						<annotation>
							<documentation>An ABSOLUTE-POINT that has its position specified with respect to the surface of the World Geodetic System 1984 (WGS 84) ellipsoid.</documentation>
						</annotation>
					</element>
					<element name="PrimaryConstructionMaterialCode" type="jc3iedm:FacilityPrimaryConstructionMaterialCode" minOccurs="0">
						<annotation>
							<documentation xml:lang="en">The specific value that represents the major material used in building the specific FACILITY.</documentation>
						</annotation>
					</element>
					<element name="HeightDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<annotation>
							<documentation xml:lang="en">The one-dimensional linear distance representing the height of a specific FACILITY.</documentation>
						</annotation>
					</element>
					<element name="FacilityType">
						<complexType>
							<complexContent>
								<extension base="jdssdm:AbstractObjectType">
									<sequence>
										<element name="CategoryCode" type="jc3iedm:FacilityTypeCategoryCode">
											<annotation>
												<documentation xml:lang="en">The specific value that represents the class of FACILITY-TYPE. It serves as a discriminator that partitions FACILITY-TYPE into subtypes.</documentation>
											</annotation>
										</element>
									</sequence>
								</extension>
							</complexContent>
						</complexType>
					</element>
					<element name="OperationalStatusCode" type="jc3iedm:FacilityStatusOperationalStatusCode">
						<annotation>
