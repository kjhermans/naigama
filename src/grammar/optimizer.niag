-- Optimization: four chars make up one quad

CHAR2QUAD       <- {
                     'char' S { HEXBYTE } S
                     'char' S { HEXBYTE } S
                     'char' S { HEXBYTE } S
                     'char' S { HEXBYTE }
                   } -> 'quad ' $1 $2 $3 $4

-- Primitives

HEXBYTE         <- [0-9a-fA-F]^2
S               <- %s+
